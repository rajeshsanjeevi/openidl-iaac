def checkout() {
    stage('Clone') {
        git credentialsId: 'github', branch: "${params.GITHUB_BRANCH}", url: 'https://github.com/rajeshsanjeevi/openidl-iaac.git'
    }
}

def awsUploadCode() {
    sh '''
        tar -czf aws-content.tar.gz -C openidl-iaac/aws/aws_resources --exclude .git .
    '''
}
